<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.ProyectoIndividual1" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/purple_700</item>
        <item name="colorPrimaryVariant">@color/black</item>
        <item name="colorOnPrimary">#E2E2E2</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>
    <style name="RegBien" parent="Theme.AppCompat">
        <item name="android:fontFamily">sans-serif-smallcaps</item>
        <item name="android:textColor">#00B000</item>
    </style>
    <style name="RegMal" parent="Theme.AppCompat">
        <item name="android:fontFamily">sans-serif-smallcaps</item>
        <item name="android:textColor">#C00000</item>
    </style>
    <style name="ModoOscuro" parent="Theme.AppCompat">
        <item name="android:title">#FFFFFF</item>
        <item name="android:textColor">#FFFFFF</item>
        <item name="android:background">#202020</item>
        <item name="android:statusBarColor" tools:targetApi="l">#505050</item>
        <item name="colorPrimary">#404060</item>
    </style>
    <style name="MenuPrincipal" parent="Theme.AppCompat">
        <item name="android:title">#A0AA50</item>
        <item name="android:textColor">#182111</item>
        <item name="android:background">#3A5AAA</item>
    </style>
    <style name="Normal" parent="Theme.AppCompat">
        <item name="colorPrimary">#222288</item>
        <item name="android:title">#AAAAAA</item>
        <item name="android:textColor">#000000</item>
        <item name="android:textSize">18dp</item>
        <item name="android:background">#5555CC</item>
        <item name="android:statusBarColor" tools:targetApi="l">#555588</item>
    </style>
    <style name="Listas" parent="Theme.AppCompat">
        <item name="colorPrimary">#CBBBCB</item>
        <item name="android:title">#000000</item>
        <item name="android:textColor">#000000</item>
        <item name="android:textSize">16dp</item>
        <item name="android:background">#AAAAAA</item>
        <item name="android:statusBarColor" tools:targetApi="l">#DDDDDD</item>
    </style>
</resources>